doi = {10.1080/10629360903568580},
issn = {1062-936X},
journal = {SAR and QSAR in Environmental Research},
keywords = {anti-cancer strategies,apoptosis,cellular automata,mitogen-activated protein kinase,modelling,networks},
month = {jan},
