month = {mar},
title = {{On the conversion of multivalued to Boolean dynamics}},
url = {http://arxiv.org/abs/1703.06746},
year = {2017}
}
@article{Participantsinthe2001ConferenceonEthicalAspectsofResearchinDevelopingCountries2002,
abstract = {The need to provide reasonable availability of a proven intervention to the inhabitants of the host community or country at the completion of a trial has been claimed to be necessary to avoid exploitation and, therefore, required for ethical research in developing countries. Not only is this requirement vague, it is seriously flawed. It mistakes the type of benefit with the need to provide a fair share of benefits to avoid exploitation. Furthermore, it applies only to a very narrow range of research, i.e., successful Phase III studies. The authors of this [Policy Forum][1] propose to move beyond the reasonable availability requirement to guarantee Fair Benefits.

