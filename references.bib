author = {Dubrova, Elena and Teslenko, Maxim},
eprint = {arXiv:0901.4448v1},
keywords = {Boolean network,SAT,attractor,bounded model checking,gene regulatory network},
title = {{A SAT-Based Algorithm for Computing Attractors in Synchronous Boolean Networks}},
url = {https://arxiv.org/pdf/0901.4448.pdf https://ieeexplore.ieee.org/document/5958722/},
year = {2009}
