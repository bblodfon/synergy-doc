pages = {181--99},
pmid = {15561791},
publisher = {American Medical Informatics Association},
title = {{Using Petri Net tools to study properties and dynamics of biological systems.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15561791 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC551550},
